   <view class="weui-panel">
            <view class="weui-panel__hd">今日排行榜</view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_small-appmsg">
                    <view class="weui-cells weui-cells_in-small-appmsg">
                        <view wx:for="{{rankList}}" wx:key="value" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                            <view class="weui-cell__bd weui-cell_primary text-warp">
                                <view class="text_left">{{item.name}}</view>
                                <view class="text_right">查询次数：{{item.value}}</view>
                            </view>
                        </view>
                      
                    </view>
                </view>
            </view>
        </view>